<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UserLogin</servlet-name>
  	<display-name>UserLogin</display-name>
  	<description></description>
  	<servlet-class>UserLogin</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DemoServlet</servlet-name>
  	<display-name>DemoServlet</display-name>
  	<description></description>
  	<servlet-class>login.DemoServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PeopleEmployee</servlet-name>
  	<display-name>PeopleEmployee</display-name>
  	<description></description>
  	<servlet-class>PeopleEmployee</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserLogin</servlet-name>
  	<url-pattern>/UserLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DemoServlet</servlet-name>
  	<url-pattern>/DemoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PeopleEmployee</servlet-name>
  	<url-pattern>/PeopleEmployee</url-pattern>
  </servlet-mapping>
</web-app>
